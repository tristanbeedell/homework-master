<form method="post" id="signup">
	<% let prevDiv, prevSet %>
	<% subjects.forEach((set) => { %>

	<% if (set.division != prevDiv){ %>
	<% if (prevDiv != undefined){ %>
	</select>
	<%}%>
	<% prevDiv = set.division %>
	<label for="<%= set.division%>">
		<%= set.division%>
	</label>
	<select name="classes[<%= set.division%>]" id="<%= set.division%>" onchange="return updateTable('<%= set.division%>', this.value)">
		<option value="none">
			Choose Class
		</option>
		<% }%>

		<% if (set.name != prevSet) {%>
		<%prevSet = set.name%>
		<option value="<%= set.name%>">
			<%= set.name%> -
			<%= set.subject%>
			<%}%>
			|
			<%= set.teacher%>
			<% if (set.name != prevSet) {%>
		</option>
		<%}%>

		<% }) %>
		<input class="important-link signup-modal" type="button" value="OK" style="grid-column: 1/-1;">


		<div id="signup-modal" class="modal">
			<div class="signup-modal slide-down">
				<div>You are about to create your account.</div>
				<span class="close">&times;</span>
				<input type="submit" value="Continue" />
			</div>
		</div>
</form>